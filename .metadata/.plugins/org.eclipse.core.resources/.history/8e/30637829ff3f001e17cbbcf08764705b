package inheritancedemo;

public class Phone {
	int cost=67570;
	String productName="Apple";
	
	public Phone() { //no arg constructor
		
	}
	public Phone(int cost) {
		this.cost = cost;
	}
	

	public Phone(String productName) {
		super();
		this.productName = productName;
	}
	
	public Phone(int cost, String productName) {
		super();
		this.cost = cost;
		this.productName = productName;
	}
	public static void main(String[] args) {
		
	}
}
