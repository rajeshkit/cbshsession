package inheritancedemo;

public class Phone {
	int cost=67570;
	String productName="Apple";
	
	public Phone() { //no arg constructor	
	}
	public Phone(int cost) {//one int arg constructor	
		this.cost = cost;
	}
	public Phone(String productName) {//one String arg constructor	
		this.productName = productName;
	}
	
	public Phone(int cost, String productName) {//two arg constructor	
		this.cost = cost;
		this.productName = productName;
	}
	public static void main(String[] args) {
		Phone p=new Phone(4656,"dfdsf");
	}
}
