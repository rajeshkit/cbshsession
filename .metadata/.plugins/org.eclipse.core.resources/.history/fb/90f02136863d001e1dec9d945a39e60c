package demo1;

public class Area {
	public static void main(String[] args) {
		String name="hsbc";
		System.out.println("code line 1"); //statement1
		System.out.println("code line 2"); //statement1
		System.out.println("code line 3"); //statement1
		if(name.equals("hsbc")) {
			System.out.println("code line 4"); //statement1
			System.out.println("code line 5"); //statement1
		}
		System.out.println("code line 6"); //statement1
		System.out.println("code line 7"); //statement1
	}
}
